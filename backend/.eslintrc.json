{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "google"
    ],
    // "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    // "plugins": [
    //     "@typescript-eslint"
    // ],
    "rules": {
        "quotes": ["warn", "single"],
        "linebreak-style": ["off", "windows"],
        "new-cap": ["off"],
        "require-jsdoc": ["off"],
        "padded-blocks": ["off"],
        "no-trailing-spaces": ["off"],
        "comma-dangle": ["off"],
        "max-len": ["off"],
        "indent": ["off"]
    }
}
